          "rawQuery": true,
          "rawSql": "CREATE extension IF NOT EXISTS cube;\nCREATE extension IF NOT EXISTS earthdistance;\n\nDO $$ \nBEGIN\nIF ${id:singlequote} = 'none' THEN\n  RAISE EXCEPTION 'No id was given, you have to navigate to this dashboard using a link from Trips dashboard';\nEND IF;\nEND $$;\n\nSELECT\n  \"start_date\" AS \"Start Date\",\n  \"destination_date\" AS \"End Date\",\n  EXTRACT('epoch' FROM(\"destination_date\"-\"start_date\")) AS \"Duration\",\n  (point(start_position_longitude, start_position_latitude)<@>point(destination_position_longitude, destination_position_latitude)) * 1.609344 as \"Distance\",\n  (\"destination_odometer\"-\"start_odometer\") AS \"Length\",\n  \"destination_odometer\" AS \"Odometer\",\n  string_agg(trips_tags.tags_name, ', ') AS \"Tags\"\nFROM trips\nLEFT JOIN trips_tags on trips.id = trips_tags.trips_id\nWHERE\n  id = $id\nGROUP BY \"start_date\", \"destination_date\", start_position_longitude, start_position_latitude, destination_position_longitude, destination_position_latitude, \"destination_odometer\", \"start_odometer\"",
