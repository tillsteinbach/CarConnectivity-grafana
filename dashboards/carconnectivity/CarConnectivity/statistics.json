          "editorMode": "code",
          "format": "table",
          "group": [],
          "hide": false,
          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "SELECT\n  EXTRACT(EPOCH FROM date_trunc('$period', \"start_date\"))*1000 AS date_from,\n  EXTRACT(EPOCH FROM date_trunc('$period', \"start_date\" + ('1 ' || '$period')::INTERVAL))*1000 AS date_to,\n  CASE '$period'\n    WHEN 'month' THEN to_char(date_trunc('month', \"start_date\" AT TIME ZONE '${__to:date:Z}'), 'YYYY Month')\n    WHEN 'year' THEN to_char(date_trunc('year', \"start_date\" AT TIME ZONE '${__to:date:Z}'), 'YYYY')\n    WHEN 'week' THEN 'week ' || to_char(date_trunc('week', \"start_date\" AT TIME ZONE '${__to:date:Z}'), 'WW') || ' starting ' || to_char(date_trunc('week', \"start_date\" AT TIME ZONE '${__to:date:Z}'), 'YYYY-MM-DD')\n    ELSE to_char(\"start_date\" AT TIME ZONE '${__to:date:Z}', 'YYYY-MM-DD')\n  END AS \"Period\",\n  COUNT(*) AS \"# Drives\",\n  EXTRACT(EPOCH FROM SUM(\"destination_date\"-\"start_date\")) AS \"Time driven\",\n  SUM(\"destination_odometer\"-\"start_odometer\") AS \"Distance\",\n  percentile_cont(0.5) WITHIN GROUP (ORDER BY (\"destination_odometer\"-\"start_odometer\")) AS \"Median Distance\"\nFROM \n  trips\nWHERE\n  $__timeFilter(\"start_date\") AND\n  trips.vin = '$VIN'\nGROUP BY \"Period\", date_from, date_to\nORDER BY \"date_from\" DESC",
