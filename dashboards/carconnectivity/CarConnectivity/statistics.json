          "group": [],
          "hide": false,
          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "SELECT\n  EXTRACT(EPOCH FROM date_trunc('$period', \"session_start_date\"))*1000 AS date_from,\n  EXTRACT(EPOCH FROM date_trunc('$period', \"session_start_date\" + ('1 ' || '$period')::INTERVAL))*1000 AS date_to,\n  CASE '$period'\n    WHEN 'month' THEN to_char(date_trunc('month', \"session_start_date\" AT TIME ZONE '${__to:date:Z}'), 'YYYY Month')\n    WHEN 'year' THEN to_char(date_trunc('year', \"session_start_date\" AT TIME ZONE '${__to:date:Z}'), 'YYYY')\n    WHEN 'week' THEN 'week ' || to_char(date_trunc('week', \"session_start_date\" AT TIME ZONE '${__to:date:Z}'), 'WW') || ' starting ' || to_char(date_trunc('week', \"session_start_date\" AT TIME ZONE '${__to:date:Z}'), 'YYYY-MM-DD')\n    ELSE to_char(\"session_start_date\" AT TIME ZONE '${__to:date:Z}', 'YYYY-MM-DD')\n  END AS \"Period\",\n  COUNT(*) AS \"# Charges\",\n  EXTRACT(EPOCH FROM SUM(\"session_end_date\"-\"session_start_date\")) AS \"Time charging\",\n  EXTRACT(EPOCH FROM AVG(\"session_end_date\"-\"session_start_date\")) AS \"Avg. charging\",\n  SUM(COALESCE(\"real_charged\", COALESCE((\"meter_end_kwh\"-\"meter_start_kwh\"),\n  (\"end_level\" - \"start_level\") * (capacity / 100::NUMERIC)))) AS \"Energy Charged\",\n  AVG(COALESCE(\"real_charged\", COALESCE((meter_end_kwh-\"meter_start_kwh\"),\n  (\"end_level\" - \"start_level\") * (capacity / 100::NUMERIC)))) AS \"Avg. Charged\",\n  SUM(\"real_cost\"::NUMERIC / 100::NUMERIC) AS \"Cost\"\nFROM \n  charging_sessions\nLEFT JOIN vehicles ON charging_sessions.vin = vehicles.vin\nLEFT JOIN drives ON vehicles.vin = drives.vin\nWHERE\n  drives.type = 'ELECTRIC' AND\n  $__timeFilter(\"session_start_date\") AND\n  charging_sessions.vin = '$VIN'\nGROUP BY \"Period\", date_from, date_to\nORDER BY \"date_from\" DESC",
          "refId": "Charging",
