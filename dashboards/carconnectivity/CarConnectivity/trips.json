          "editorMode": "code",
          "format": "table",
          "group": [],
          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "WITH formatted_locations AS (\n  SELECT\n    t.id,\n    t.vin,\n    t.start_location_uid,\n    t.destination_location_uid,\n    t.start_date,\n    t.destination_date,\n    \n    COALESCE(\n      sl.display_name,\n      CONCAT_WS(', ',\n        CONCAT_WS(', ', sl.name, sl.amenity),\n        NULLIF(CONCAT_WS(' ', sl.road, sl.house_number), ''),\n        sl.neighbourhood,\n        sl.city,\n        sl.postcode,\n        sl.country\n      )\n    ) AS start_location_formatted,\n\n    COALESCE(\n      dl.display_name,\n      CONCAT_WS(', ',\n        CONCAT_WS(', ', dl.name, dl.amenity),\n        NULLIF(CONCAT_WS(' ', dl.road, dl.house_number), ''),\n        dl.neighbourhood,\n        dl.city,\n        dl.postcode,\n        dl.country\n      )\n    ) AS destination_location_formatted\n\n  FROM trips t\n  LEFT JOIN locations sl ON t.start_location_uid = sl.uid\n  LEFT JOIN locations dl ON t.destination_location_uid = dl.uid\n\n  WHERE\n    t.vin = '$VIN'\n    AND ($__timeFilter(t.start_date) OR $__timeFilter(t.destination_date))\n    AND t.start_location_uid IS NOT NULL\n    AND t.destination_location_uid IS NOT NULL\n    ${tag_sql:raw}\n)\n\nSELECT\n  fl.start_location_formatted AS \"Start\",\n  fl.destination_location_formatted AS \"Destination\",\n  COUNT(DISTINCT fl.id) AS \"# Drives\",\n  PERCENTILE_CONT(0.5) WITHIN GROUP (\n    ORDER BY EXTRACT(EPOCH FROM (fl.destination_date - fl.start_date))\n  ) AS \"Med. Duration\",\n  fl.start_location_uid,\n  fl.destination_location_uid\n\nFROM formatted_locations fl\n\nGROUP BY\n  fl.start_location_uid,\n  fl.destination_location_uid,\n  fl.start_location_formatted,\n  fl.destination_location_formatted\n\nORDER BY\n  \"# Drives\" DESC,\n  \"Start\" ASC\n\nLIMIT 10;\n",
          "refId": "A",
